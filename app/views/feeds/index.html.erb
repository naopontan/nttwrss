<h1>Listing feeds</h1>

<table>
  <tr>
    <th>My request</th>
    <th>Title</th>
    <th>Link</th>
    <th>Description</th>
    <th>Pub date</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @feeds.each do |feed| %>
  <tr>
    <td><%= feed.my_request %></td>
    <td><%= feed.title %></td>
    <td><%= feed.link %></td>
    <td><%= feed.description %></td>
    <td><%= feed.pub_date %></td>
    <td><%= feed.content %></td>
    <td><%= link_to 'Show', feed %></td>
    <td><%= link_to 'Edit', edit_feed_path(feed) %></td>
    <td><%= link_to 'Destroy', feed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Feed', new_feed_path %>
